{
  "manifest_version": 3,
  "name": "IDM Playlist Connector",
  "version": "1.0",
  "description": "Send YouTube Playlists to IDM via Local Server",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "idm_icon.png",
      "48": "idm_icon.png",
      "128": "idm_icon.png"
    }
  },
  "icons": {
    "16": "idm_icon.png",
    "48": "idm_icon.png",
    "128": "idm_icon.png"
  }
}